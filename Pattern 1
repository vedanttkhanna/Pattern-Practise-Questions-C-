#include <iostream>
using namespace std;
int main(){
    int i;
    int j;
    int n;
    cin >> n;
   for(i=0;i<n;i++){
       for(j=0;j<=i;j++){
           if(i%2==0 and j%2==0 or i%2!=0 and j%2!=0){
               cout<<"1";
           }
           else{
               cout<<"0";
           }
       }
       cout<<endl;
   }
    return 0;
}
